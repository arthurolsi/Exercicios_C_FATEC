#include <stdio.h>

// Soma vetor

int main()
{
    int vetorA[5], vetorB[5], vetorC[5];
    int i = 0;
    int teste = 0;
    
    for (i = 0; i < 5; i++){
        printf("Digite um valor para A\n");
        scanf("%d", &vetorA[i]);
        printf("Digite um valor para B\n");
        scanf("%d", &vetorB[i]);
    }
    
    printf("O vetor 1 fica: \n");
    for (i = 0; i < 5; i++){
        printf("%d-", vetorA[i]);
    }
    
    printf("\nO vetor 2 fica: \n");
    for (i = 0; i < 5; i++){
        printf("%d-", vetorB[i]);
    }
    
    printf("\nA soma entre os itens de mesmo Ã­ndice do vetor A e B fica: \n");
    for (i = 0; i < 5; i++){
        vetorC[i] = vetorA[i] + vetorB[i];
        printf("%d-", vetorC[i]);
    }
    return 0;
}

