#include <stdio.h>

// 3) Desenvolva um algoritmo que calcule e exiba a soma dos mil primeiros números inteiros
// Atividade realizada em 11/05/2022

int main()
{
    int i = 0;
    int soma = 0;

    for (i = 0; i <= 1000; i++){
        soma = soma + i;
    }
    
    printf("A soma é: %d", soma);


    return 0;
}

