#include <stdio.h>

// Desafio matriz 2.1
// Atividade realizada em 06/03/2023

int main()
{
    char matriz [10][10];
    int contador1 = 0;
    int contador2 = 0;
    
    for(contador1 = 0; contador1 <= 9; contador1++){
        for(contador2 = 0; contador2 <= 9; contador2++){
           matriz[contador1][contador2] = '-'; 
        }
    }
    
    matriz[1][3]='X';
    matriz[2][3]='X';
    matriz[3][3]='X';
    matriz[4][3]='X';
    matriz[5][3]='X';
    matriz[6][3]='X';
    matriz[7][3]='X';
    matriz[8][3]='X';
    
    
    
    for(contador1 = 0; contador1 <= 9; contador1++){
        for(contador2 = 0; contador2 <= 9; contador2++){
           printf("%c",matriz[contador1][contador2]); 
        }
        printf("\n");
    }
    return 0;
}

