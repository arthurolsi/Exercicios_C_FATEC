#include <stdio.h>

// 4)Faça um programa que dado um valor de salário apresente um aumento de 10%
// Atividade realizada em 10/05/2022

int main()
{
    int salario, resultado, aumento;
    
    printf("Informe o salário \n");
    scanf("%d", &salario);
    
    aumento = salario * 0.10;
    resultado = salario + aumento;
    printf("O salario com um aumento de 10 por cento é: \n");
    printf("%d", resultado);

    return 0;
}

