#include <stdio.h>

// Matriz 3x3
// Atividade realizada em 01/06/2022

int main()
{
    int matrizA[3][3], i, contador2;
    
    
    // INSERIR
    for(i = 0; i < 3; i++){
        for(contador2 = 0; contador2 < 3; contador2++){
        printf("Digite um número para a posição\n");
        printf("%d", i + 1);
        printf(" - ");
        printf("%d\n", contador2 + 1);
        scanf("%d",&matrizA[i][contador2]);
        }
    }
    
    // ESCREVER
    for(i = 0; i < 3; i++){
        for(contador2 = 0; contador2 < 3; contador2++){
        printf(" %d ",matrizA[i][contador2]);
        }
    printf("\n");
    }
    return 0;
}

