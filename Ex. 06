#include <stdio.h>

/* 1) Dado um numero apresentar a tabuada do mesmo */
// Atividade realizada em 11/05/2022

int main()
{
    int numero;
    int i;
    int result;
    
    printf("Digite um n√∫mero ");
    scanf("%d", &numero);
    
    for (i = 0; i < 11; i++)
    {
        printf("\n %d", numero);
        printf(" X ");
        printf("%d", i);
        printf(" = ");
        result = numero * i;
        printf("%d", result);
    }

    return 0;
}

